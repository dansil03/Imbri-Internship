{
  "kafka_server": "kafka:29092",
  "kafka_topic": "UltimoData",
  "kafka_consumer_group": "UltimoGroup",
  "dgraph_url": "http://internship-project-core-alpha-1:8080/mutate?commitNow=true",
  "unique_properties": {
    "Ticket": "issue_number",
    "User": "username",
    "Organization": "organization_name", 
    "Email": "email_address", 
    "Phone": "phone_number"
  },
  "api_to_property_mapping": {
    "issue_number": "Code",
    "reported_at": "Melddatum",
    "due_date": "Gepl. gereeddatum",
    "closed_at": "Werkelijk gereed",
    "first_name": "Melder", 
    "username": "Melder",
    "organization_name": "Gebouw",
    "email_address": "E-mailadres melder",
    "phone_number": "Telefoon"
  },
  "entity_definitions": {
    "Ticket": {
      "prefix": "_:Ticket",
      "properties": {
        "issue_number": "issue_number",
        "reported_at": "reported_at",
        "due_date": "due_date",
        "closed_at": "closed_at"
      },
      "relations": {
        "Organization": "organization_id",
        "User": "reported_by"
      }
    },
    "User": {
      "prefix": "_:User", 
      "properties": {
        "first_name": "first_name",
        "username": "username"
      },
      "relations": {
        "Contact": "contacts"
      }
    },
    "Organization": {
      "prefix": "_:Org",
      "properties": {
        "organization_name": "organization_name"
      }
    },
    "Contact": {
      "prefix": "_:Contact",
      "match_properties": ["email_address", "phone_number"],
      "relations": {
        "Email": "email", 
        "Phone": "phone"
      }
    }, 
    "Email": {
      "prefix": "_:Email",
      "properties": {
        "email_address": "email_address"
      }
    }, 
    "Phone": {
      "prefix": "_Phone", 
      "properties": {
        "phone_number": "phone_number" 
      }
    }
  },
  "mappings": {
    "priority_mapping": {
      "1": "Critical",
      "2": "High",
      "3": "Medium",
      "4": "Medium",
      "5": "Low"
    },
    "status_mapping": {
      "Gesloten": "Closed",
      "Open": "Open"
    }
  }
}
